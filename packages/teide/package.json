{
  "name": "@lab009/teide",
  "version": "3.1.0",
  "description": "Toolkit for building redux/react applications",
  "repository": "lab009/teide",
  "bugs": {
    "url": "https://github.com/lab009/teide/issues"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "scripts": {
    "build": "yarn run build:commonjs && yarn run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --ignore **/__tests__ ./src -d ./dist",
    "build:es": "cross-env BABEL_ENV=es  babel --ignore **/__tests__ ./src -d ./es",
    "clean": "rimraf ./dist && rimraf ./es && rimraf ./coverage && rimraf ./flow-coverage",
    "preversion": "yarn run clean && yarn run build",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:coverage": "yarn run test -- --coverage",
    "test:coverage:deploy": "yarn run test:coverage && codecov"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "lodash.filter": "4.6.0",
    "lodash.foreach": "4.5.0",
    "lodash.map": "4.6.0",
    "lodash.mapvalues": "4.6.0",
    "lodash.reduce": "4.6.0",
    "lodash.values": "4.3.0",
    "prop-types": "15.5.8",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.4",
    "reduce-reducers": "0.1.2",
    "redux": "3.6.0",
    "redux-actions": "2.0.2",
    "redux-batched-subscribe": "0.1.6",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@lab009/babel-preset-magma": "3.3.3",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "codecov": "2.1.0",
    "cross-env": "4.0.0",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "jest": "19.0.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.1",
    "sinon": "2.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|coverage|flow-typed|node_modules|tools|es)/"
    ]
  }
}

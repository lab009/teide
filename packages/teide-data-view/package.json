{
  "name": "@lab009/teide-data-view",
  "version": "3.3.0",
  "description": "Lightweight data view abstraction for managing fetching/loaded/errored states for remote data",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "keywords": [
    "react",
    "redux",
    "data"
  ],
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MPL-2.0",
  "repository": "https://github.com/lab009/teide/tree/master/packages/teide-data-view",
  "bugs": "https://github.com/lab009/teide/issues",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^6.9.0 || ^8 || >=9",
    "npm": ">=5.4.0",
    "yarn": ">=1.2.1"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:commonjs && yarn run build:es",
    "clean": "rimraf ./dist && rimraf ./es && rimraf ./coverage && rimraf ./flow-coverage",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --ignore **/__tests__ ./src -d ./dist",
    "build:es": "cross-env BABEL_ENV=es  babel --ignore **/__tests__ ./src -d ./es",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:coverage": "yarn run test --coverage",
    "test:coverage:deploy": "yarn run test:coverage && codecov"
  },
  "peerDependencies": {
    "@lab009/teide": "3.2.0"
  },
  "dependencies": {
    "immutable": "3.8.2"
  },
  "devDependencies": {
    "@lab009/babel-preset-magma": "3.4.3",
    "@lab009/teide": "^3.3.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "codecov": "3.0.2",
    "cross-env": "5.2.0",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.4",
    "jest": "23.2.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "sinon": "6.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|coverage|flow-typed|node_modules|tools|es)/"
    ]
  }
}

{
  "name": "@lab009/teide-router",
  "version": "3.0.2",
  "description": "Dead simple router for @lab009/teide",
  "repository": "lab009/teide",
  "bugs": {
    "url": "https://github.com/lab009/teide/issues"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "router"
  ],
  "scripts": {
    "build": "yarn run build:commonjs && yarn run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --ignore **/__tests__ ./src -d ./dist",
    "build:es": "cross-env BABEL_ENV=es  babel --ignore **/__tests__ ./src -d ./es",
    "clean": "rimraf ./dist && rimraf ./es && rimraf ./coverage && rimraf ./flow-coverage",
    "release": "yarn run clean && yarn run build",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:coverage": "yarn run test -- --coverage",
    "test:coverage:deploy": "yarn run test:coverage && codecov"
  },
  "peerDependencies": {
    "@lab009/teide": "3.1.0",
    "react": "^15.0.0"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "react-router": "2.8.1",
    "react-router-redux": "3.0.0",
    "react-router-scroll": "0.3.3"
  },
  "devDependencies": {
    "@lab009/babel-preset-magma": "3.1.1",
    "@lab009/teide": "3.1.0",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-jest": "19.0.0",
    "codecov": "1.0.1",
    "cross-env": "3.2.4",
    "enzyme": "2.7.1",
    "enzyme-to-json": "1.5.0",
    "jest": "19.0.2",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "rimraf": "2.6.1",
    "sinon": "1.17.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|coverage|flow-typed|node_modules|tools|es)/"
    ]
  }
}
